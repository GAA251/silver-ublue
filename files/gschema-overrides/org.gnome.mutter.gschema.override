[org.gnome.mutter]
# Make GNOME center all new windows
center-new-windows=true
# Enable experimental features for VRR etc. (https://github.com/ublue-os/bazzite/blob/main/spec_files/mutter/org.gnome.mutter.fedora.gschema.override)
experimental-features=['variable-refresh-rate', scale-monitor-framebuffer', 'xwayland-native-scaling', 'kms-modifiers']

